<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>powerline</string>

		<key>EnvironmentVariables</key>
		<dict>
			<key>LC_ALL</key>
			<string>en_US.utf8</string>
			<key>LANG</key>
			<string>en_US.utf8</string>
		</dict>

		<key>ProgramArguments</key>
		<array>
			<string>/usr/local/bin/powerline-daemon</string>
			<string>--quiet</string>
		</array>

		<key>RunAtLoad</key>
		<true/>
		<key>KeepAlive</key>
		<true/>
	</dict>
</plist>
